#Лаба 9 питон
def can_form_parallelogram(a, b, c, d):
    if (a == c and b == d) or (a == b and c == d):
        return True
    else:
        return False
a = int(input("Введите длину отрезка a: "))
b = int(input("Введите длину отрезка b: "))
c = int(input("Введите длину отрезка c: "))
d = int(input("Введите длину отрезка d: "))
if can_form_parallelogram(a, b, c, d):
    print("Из этих отрезков можно построить параллелограмм.")
else:
    print("Из этих отрезков нельзя построить параллелограмм.")
